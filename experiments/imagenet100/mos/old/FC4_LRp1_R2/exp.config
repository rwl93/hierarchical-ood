# Main hierarchy skeleton filled w/ default values
# Main {{{
model: MOS
## Model parameters {{{
## NOTE: this is a oneof and currently supports:
#  - softmax_mc
#  - ilr_mc
#  - hilr_mc
#  - cascade_mc
#  - cascadefchead_mc
#  - softmaxfchead_mc
#  - mos_mc
mos_mc {
  fc_head_sizes: 512
  fc_head_sizes: 256 
  fc_head_sizes: 128
}
## }}}
data_dir: 'data/coarse'
hierarchy_fn: 'two-lvl-wn.pth'
backbone: 'resnet50'
## Training Loop Parameters {{{
train_params {
  epochs: 90
  batch_size: 128
  checkpoint_fn: ''
  log_fn: ''
}
## }}}
## Optimizer parameters {{{
## NOTE: this is a oneof but only currently supports sgd
sgd {
  learning_rate: 0.1
  momentum: 0.9
  weight_decay: 1e-4
  nesterov: false
}
## }}}
## Loss parameters {{{
## NOTE: this is a oneof and currently supports: ce,bce,hbce,hl
## NOTE: the only option that has subfields is hl
ml {}
## }}}
seed: 9084345
repeat_iters: 1
no_save: false
verbose: false
savedir: ''
far_ood_dsets: 'iNaturalist'
far_ood_dsets: 'SUN'
far_ood_dsets: 'Places'
far_ood_dsets: 'Textures'
# }}}
